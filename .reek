FeatureEnvy:
  exclude:
   - 'track_registration'
IrresponsibleModule:
  enabled: false
TooManyMethods:
  exclude:
    - Users::ConfirmationsController
UnusedPrivateMethod:
  enabled: true
  exclude:
    - ActiveJob::Logging::LogSubscriber
UtilityFunction:
  exclude:
    - AnalyticsEventJob#perform
exclude_paths:
  - db/migrate
  - spec/**/*
