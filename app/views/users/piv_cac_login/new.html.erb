<% title @presenter.title %>

<h1 class='h3 margin-y-0'>
  <%= @presenter.heading %>
</h1>

<p class='margin-top-2 margin-bottom-2'>
  <%= raw @presenter.info %>
</p>

<%= render 'shared/piv-cac-button' do %>
  <%= link_to @presenter.piv_cac_capture_text,
              @presenter.piv_cac_service_link,
              class: 'btn btn-primary' %>
<% end %>

<%= render 'shared/cancel', link: new_user_session_url %>
