<% keys = YAML.load_file Rails.root.join('config', 'js_locale_strings.yml') %>

window.LoginGov.I18n.strings.es = {};
<% keys.each do |key| %>
  window.LoginGov.I18n.strings.es['<%= ActionController::Base.helpers.j key %>'] = '<%= ActionController::Base.helpers.j I18n.t(key, locale: 'es') %>';
<% end %>
