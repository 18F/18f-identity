window.LoginGov = window.LoginGov || {};
window.LoginGov.assets = {};

<% keys = [
  'close-white-alt.svg',
  'id-card.svg',
  'spinner.gif',
  'spinner@2x.gif'
] %>

<% keys.each do |key| %>
  window.LoginGov.assets['<%= ActionController::Base.helpers.j key %>'] = '<%= ActionController::Base.helpers.j ActionController::Base.helpers.asset_path key %>';
<% end %>
